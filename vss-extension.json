{
  "manifestVersion": 1,
  "id": "snazzy-operator-plus",
  "version": "1.2.0",
  "name": "Snazzy Operator Plus",
  "publisher": "bsides",
  "description": "Visual Studio Code theme based on hyper-snazzy and optimised for Operator Mono.",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "images/icon.png"
  },
  "categories": ["Themes"],
  "tags": [
    "color-theme",
    "operator",
    "snazzy",
    "operator mono",
    "snazzy theme",
    "snazzy operator plus"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Snazzy Operator",
        "uiTheme": "vs-dark",
        "path": "./themes/snazzy-operator-plus-color-theme.json"
      }
    ]
  },
  "screenshots": [
    {
      "path": "preview.png"
    }
  ],
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE"
    },
    "changelog": {
      "path": "CHANGELOG.md"
    }
  },
  "links": {
    "home": {
      "uri": "https://github.com/bsides/snazzy-operator-plus"
    },
    "support": {
      "uri": "https://github.com/bsides/snazzy-operator-plus/issues"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/bsides/snazzy-operator-plus"
  },
  "files": [
    {
      "path": "themes/snazzy-operator-plus-color-theme.json"
    }
  ]
}
